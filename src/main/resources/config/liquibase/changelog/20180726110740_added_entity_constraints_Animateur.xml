<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Animateur.
    -->
    <changeSet id="20180726110740-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="acteur_type_animateur_id"
                                 baseTableName="animateur"
                                 constraintName="fk_animateur_acteur_type_animateur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="acteur_type"/>

        <addForeignKeyConstraint baseColumnNames="agreement_id"
                                 baseTableName="animateur"
                                 constraintName="fk_animateur_agreement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agreement"/>

        <addForeignKeyConstraint baseColumnNames="animateur_fournisseur_id"
                                 baseTableName="animateur"
                                 constraintName="fk_animateur_animateur_fournisseur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="animateur_fournisseur"/>

    </changeSet>
</databaseChangeLog>
